# Configurações de produção
VITE_APP_ENV=production
VITE_APP_NAME="Sistema de Gestão Vitana"
VITE_APP_VERSION="2.0.0"

# URLs da aplicação (Railway irá definir automaticamente)
VITE_APP_URL=$RAILWAY_STATIC_URL
VITE_API_URL=$RAILWAY_STATIC_URL/api

# Configurações de segurança
VITE_SECURE_MODE=true
VITE_ENABLE_ANALYTICS=true

# Configurações de cache
VITE_CACHE_DURATION=86400